<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="588.0" prefWidth="555.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Passenger.ParkingController">
   <children>
      <Label alignment="CENTER" layoutX="182.0" layoutY="42.0" prefHeight="26.0" prefWidth="192.0" text="Parking Reservation">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="101.0" layoutY="128.0" prefHeight="23.0" prefWidth="130.0" text="Name">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="260.0" layoutY="126.0" prefHeight="27.0" prefWidth="194.0" promptText="First and Last Name" />
      <Label alignment="CENTER" layoutX="105.0" layoutY="254.0" prefHeight="23.0" prefWidth="130.0" text="Email">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="emailField" layoutX="260.0" layoutY="252.0" prefHeight="27.0" prefWidth="194.0" promptText="Email" />
      <Label alignment="CENTER" layoutX="107.0" layoutY="317.0" text="Check in date">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <DatePicker fx:id="CheckinDatePicker" layoutX="260.0" layoutY="315.0" promptText="mm/dd/yyyy" />
      <Label alignment="CENTER" layoutX="65.0" layoutY="393.0" prefHeight="23.0" prefWidth="185.0" text="Parking Stall # :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Line endX="27.0" layoutX="357.0" layoutY="416.0" startX="-100.0" />
      <Button layoutX="407.0" layoutY="387.0" mnemonicParsing="false" onAction="#search" prefHeight="35.0" prefWidth="80.0" text="Search">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="nameError" alignment="CENTER" layoutX="260.0" layoutY="101.0" prefHeight="17.0" prefWidth="194.0" />
      <Label fx:id="emailError" alignment="CENTER" layoutX="232.0" layoutY="223.0" prefHeight="23.0" prefWidth="250.0" />
      <Button layoutX="143.0" layoutY="472.0" mnemonicParsing="false" onAction="#addParking" prefHeight="35.0" prefWidth="250.0" text="Submit">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="closeButton" layoutX="143.0" layoutY="522.0" mnemonicParsing="false" onAction="#closeButtonOnAction" prefHeight="35.0" prefWidth="250.0" text="Close">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="parkingLabel" alignment="CENTER" layoutX="257.0" layoutY="388.0" prefHeight="26.0" prefWidth="128.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="419.0" layoutY="522.0" mnemonicParsing="false" onAction="#clearReserveForm" prefHeight="35.0" prefWidth="93.0" text="Clear" />
      <Label alignment="CENTER" layoutX="103.0" layoutY="191.0" prefHeight="23.0" prefWidth="130.0" text="ID Number">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="idField" layoutX="260.0" layoutY="189.0" prefHeight="27.0" prefWidth="194.0" promptText="ID #" />
   </children>
